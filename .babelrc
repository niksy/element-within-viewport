{
	"presets": [
		"babel-preset-niksy/next",
		["babel-preset-niksy", {
			"@babel/preset-env": {
				"loose": true
			}
		}]
	],
	"plugins": [
		"@babel/plugin-transform-object-assign"
	],
	"env": {
		"test": {
			"presets": [
				["babel-preset-niksy", {
					"@babel/preset-env": {
						"useBuiltIns": "usage"
					}
				}]
			],
			"plugins": [
				["@babel/plugin-transform-runtime", {
					"corejs": false,
					"helpers": true,
					"regenerator": false,
					"useESModules": true
				}]
			]
		}
	}
}
